{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "ReBuild (Debug)",
            "program": "${workspaceFolder}/src/App.Console/bin/Debug/netcoreapp2.0/App.Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/App.Console",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}